udepartment=[]
for(let dept in obj.department){
    let cdepartment = {
        department : dept.department
        id:dept.id,
        isChecked:true
    }
    udepartment.push(cdepartment)
}
arrayname = obj.department.map(dept=>dept.department)
let uncdepartment = initialValue.departmentValue
for(dept in obj.department){
    if(arrayname.includes(dept.department)){
        let newdept = {
            department:dept.department
            id:dept.id
            isChecked:true
        }
        initialValue.departmentValue.filter(dep=>dep.department == newdept.department).push(newdept)
    }
}